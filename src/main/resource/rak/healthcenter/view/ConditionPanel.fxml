		<ColumnConstraints hgrow="NEVER" />
	</columnConstraints>
	<children>
		<Label fx:id="title" alignment="CENTER" text="AREA" GridPane.halignment="LEFT">
         <font>
            <Font size="14.0" />
         </font></Label>
      <GridPane fx:id="labelGrid" GridPane.rowIndex="1">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
      </GridPane>
	</children>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
</GridPane>
