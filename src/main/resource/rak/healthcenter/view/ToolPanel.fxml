<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<GridPane onMouseClicked="#selectTool" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
	<columnConstraints>
		<ColumnConstraints hgrow="NEVER" />
      <ColumnConstraints hgrow="NEVER" minWidth="10.0" />
	</columnConstraints>
	<children>
		<Label fx:id="title" alignment="CENTER" text="AREA" GridPane.columnSpan="2" GridPane.halignment="CENTER">
         <font>
            <Font size="14.0" />
         </font></Label>
      <Label text="Zoom" GridPane.rowIndex="1" />
      <Label fx:id="zoom" text="ZoomValue" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Label text="System" GridPane.rowIndex="2" />
      <Label fx:id="system" text="SystemValue" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <GridPane fx:id="labelGrid" GridPane.rowIndex="3">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
      </GridPane>
	</children>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
   </rowConstraints>
</GridPane>
